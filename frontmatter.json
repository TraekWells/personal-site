{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "blog",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "Draft?",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "Created At",
          "name": "createdAt",
          "type": "datetime"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "choice",
          "choices": ["JavaScript", "Life", "Personal", "User Experience"]
        },
        {
          "title": "Type",
          "name": "type",
          "default": "blog",
          "hidden": true,
          "type": "choice",
          "choices": ["blog", "project", "journal"]
        }
      ]
    },
    {
      "name": "project",
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "Preview Image",
          "name": "previewImage",
          "type": "image"
        },
        {
          "title": "Featured Image",
          "name": "featuredImage",
          "type": "image"
        },
        {
          "title": "Link to Project",
          "name": "linkToProject",
          "type": "string"
        },
        {
          "title": "Technology Used",
          "name": "technology",
          "type": "choice",
          "choices": ["WordPress", "JavaScript"]
        },
        {
          "title": "Featured?",
          "name": "featured",
          "type": "boolean"
        },
        {
          "title": "Draft?",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "Type",
          "name": "type",
          "default": "project",
          "hidden": true,
          "type": "choice",
          "choices": ["blog", "project", "journal"]
        }
      ]
    },
    {
      "name": "journal",
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "Draft?",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "Created At",
          "name": "createdAt",
          "type": "datetime"
        },
        {
          "title": "Type",
          "name": "type",
          "default": "journal",
          "hidden": true,
          "type": "choice",
          "choices": ["blog", "project", "journal"]
        }
      ]
    }
  ],
  "frontMatter.framework.id": "next",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.preview.host": "http://localhost:3000",
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/posts/blog"
    },
    {
      "title": "journal",
      "path": "[[workspace]]/src/posts/journal"
    },
    {
      "title": "projects",
      "path": "[[workspace]]/src/posts/projects"
    }
  ],
  "frontMatter.taxonomy.tags": [
    "JavaScript",
    "Life",
    "Personal",
    "User Experience"
  ]
}
