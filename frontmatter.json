{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "blog",
      "pageBundle": false,
      "previewPath": null,
      "filePrefix": null,
      "fields": [
        {
          "title": "Draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "Created At",
          "name": "createdAt",
          "type": "datetime"
        },
        {
          "title": "Updated At",
          "name": "updatedAt",
          "type": "datetime"
        }
      ]
    },
    {
      "name": "project",
      "pageBundle": false,
      "filePrefix": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "Preview Image",
          "name": "previewImage",
          "type": "image"
        },
        {
          "title": "Featured Image",
          "name": "featuredImage",
          "type": "image"
        },
        {
          "title": "Technology",
          "name": "technology",
          "type": "list"
        },
        {
          "title": "Featured",
          "name": "featured",
          "type": "boolean"
        },
        {
          "title": "Draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Link To Code",
          "name": "linkToCode",
          "type": "string"
        },
        {
          "title": "Link To Project",
          "name": "linkToProject",
          "type": "string"
        }
      ]
    },
    {
      "name": "page",
      "pageBundle": false,
      "filePrefix": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Summary",
          "name": "summary",
          "type": "string"
        }
      ]
    },
    {
      "name": "book",
      "pageBundle": false,
      "filePrefix": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "Summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "Review",
          "name": "review",
          "type": "string"
        },
        {
          "title": "Book Cover",
          "name": "bookCover",
          "type": "image"
        },
        {
          "title": "Rating",
          "name": "rating",
          "type": "number",
          "min": 1,
          "max": 5,
          "numberOptions": {
            "step": ".25",
            "isDecimal": true
          }
        },
        {
          "title": "Date Read",
          "name": "dateRead",
          "type": "datetime",
          "format": "date"
        },
        {
          "title": "Currently Reading?",
          "name": "currentlyReading",
          "type": "boolean"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "next",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.preview.host": "http://localhost:3000",
  "frontMatter.content.pageFolders": [
    {
      "title": "Blog",
      "path": "[[workspace]]/content/blog"
    },
    {
      "title": "Journal",
      "path": "[[workspace]]/content/journal"
    },
    {
      "title": "Projects",
      "path": "[[workspace]]/content/projects"
    },
    {
      "title": "Pages",
      "path": "[[workspace]]/content/pages"
    },
    {
      "title": "Books",
      "path": "[[workspace]]/content/books"
    }
  ],
  "frontMatter.content.defaultFileType": "mdx",
  "frontMatter.git.enabled": true,
  "frontMatter.media.publicFolder": "public",
  "frontMatter.media.supportedMimeTypes": ["image/*"]
}
